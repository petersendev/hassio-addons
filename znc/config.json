{
  "name": "znc",
  "version": "0.4.0",
  "slug": "znc",
  "legacy": false,
  "maintenance": {
    "github_release": "https://github.com/linuxserver/docker-znc",
    "version_regex": "(\\d+\\.\\d+\\.\\d+-?\\w*)-(ls\\d+)"
  },
  "description": "IRC network bouncer",
  "url": "https://github.com/petersendev/hassio-addons/znc",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "6500/tcp": 6500,
    "6501/tcp": 6501
  },
  "ports_description": {
    "6500/tcp": "no SSL",
    "6501/tcp": "SSL"
  },
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:6500]",
  "boot": "auto",
  "options": {
    "PUID": 1001,
    "PGID": 1001
  },
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?"
  },
  "image": "petersendev/hassio-znc-{arch}"
}
